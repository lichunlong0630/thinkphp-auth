{
    "name": "lichunlong0630/phptools",
    "description": "auth package for thinkphp6",
    "homepage": "https://github.com/lichunlong0630/phptools",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "version": "1.0.0",
    "authors": [
        {
            "name": "chunlong.li",
            "email": "lichunlong0630@163.com"
        }
    ],
    "support": {
        "issues": "https://github.com/lichunlong0630/phptools/issues"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src/"
        }
    },
    "extra": {
        "think": {
            "config": {
                "auth": "src/config/auth.php",
                "jump": "src/config/jump.php"
            }
        }
    }
}
